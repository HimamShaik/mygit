class square_pat_8
{
	public static void main(String[] args) 
	{
		int n=5;
		for (int i=1;i<=n;i++ )
		{
			for (int j=1;j<=n ;j++ )
			{
				if(i==1 || j==1 || i==n || j==n || i==j || i+j==n+1) //(i==2 && j==3 || i==3 && j==2 || i==3 && j==4 || i==4 && j==3)
				{
			      System.out.print("* ");                                 //System.out.print("  ");
				}
				else
				{
				 System.out.print("  ");                                 //System.out.print("* ");
				}
			}
			System.out.println();
		}
	}
}
