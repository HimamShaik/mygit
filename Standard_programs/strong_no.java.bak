class strong_no 
{
	public static void main(String[] args) 
	{
		int n=145;
		
		int prod=1;
		int sum=0;
		int temp=n;
		while(n>0)
		{
			int fact=1;
		 prod=n%10;
		 
		 for (int i=1;i<=prod ;i++ )
			{
			 fact=fact*i;
			 
			}
		 sum=sum+fact;
		 n=n/10;
		}
		if (temp==sum)
		{
		 System.out.println(sum+" is strong no");

		}
		else
		{
		System.out.println(sum+"not a strong no");

		}
	}
}
